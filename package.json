{
  "name": "desci-composedb",
  "version": "1.0.0",
  "description": "A reference protocol implementation on Ceramic and ComposeDB",
  "license": "MIT",
  "author": "Edvard HÃ¼binette",
  "type": "module",
  "scripts": {
    "generate": "node scripts/generate.mjs",
    "dev": "node --no-warnings=ExperimentalWarning scripts/run.mjs",
    "deployComposites": "node scripts/composites.mjs",
    "populate": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/populate.ts",
    "ceramic": "ceramic daemon --config composedb.config.json",
    "kill": "pkill --full \"ceramic daemon|ipfs daemon\"",
    "test": "vitest --run --config vitest.config.ts",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ceramicnetwork/cli": "^2.35.0",
    "@ceramicnetwork/streamid": "^2.17.0",
    "@composedb/cli": "^0.5.0",
