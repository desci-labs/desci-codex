{
  "name": "@desci-labs/desci-codex",
  "version": "1.0.0",
  "description": "An implementation of DeSci Codex on Ceramic & ComposeDB",
  "license": "MIT",
  "author": "Edvard HÃ¼binette",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json",
    "generate": "node scripts/generate.mjs",
    "dev": "node --no-warnings=ExperimentalWarning scripts/run.mjs",
    "deployComposites": "node scripts/composites.mjs",
    "populate": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/populate.ts",
    "ceramic": "ceramic daemon --config composedb.config.json",
    "kill": "pkill --full \"ceramic daemon|ipfs daemon\"",
    "test": "vitest --run --config vitest.config.ts",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ceramicnetwork/cli": "^3.0.0",
    "@ceramicnetwork/common": "^3.0.4",
    "@ceramicnetwork/http-client": "^3.0.0",
    "@ceramicnetwork/streamid": "^3.0.0",
    "@composedb/cli": "^0.6.0",
    "@composedb/devtools": "^0.6.0",
    "@composedb/devtools-node": "^0.6.0",
    "@composedb/types": "^0.6.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "dids": "^4.0.4",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "key-did-provider-ed25519": "^3.0.2",
    "key-did-resolver": "^3.0.0",
    "ora": "^7.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2",
    "uint8arrays": "^4.0.6",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.5",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "dependencies": {
    "@composedb/client": "^0.6.0",
    "gql-query-builder": "^3.8.0",
    "graphql": "^16.8.0"
  }
}
