{
  "name": "@desci-labs/desci-codex-node-metrics",
  "version": "1.0.0",
  "description": "Codex node metrics collector",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc --build tsconfig.json",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "dev:debug": "NODE_ENV=development tsx --inspect-brk src/index.ts",
    "clean": "tsc --build --clean",
    "db:migrate": "node dist/migrations/migrate.js",
    "db:seed": "node dist/migrations/seed.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.0",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^8.0.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "pino-std-serializers": "^7.0.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1"
  }
}
