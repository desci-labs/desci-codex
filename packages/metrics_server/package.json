{
  "name": "@desci-labs/desci-codex-node-metrics",
  "version": "1.0.0",
  "description": "Codex node metrics collector",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "dev:debug": "NODE_ENV=development tsx --inspect-brk src/index.ts",
    "test": "vitest run",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:erd": "tsx scripts/generate-dbml.ts",
    "db:legacy:migrate": "node dist/migrations/migrate.js"
  },
  "devDependencies": {
    "@dbml/cli": "^5.0.0",
    "@softwaretechnik/dbml-renderer": "^1.0.31",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.0",
    "drizzle-dbml-generator": "^0.10.0",
    "drizzle-kit": "^0.31.6",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@codex/metrics": "workspace:*",
    "@libp2p/crypto": "^5.1.7",
    "@libp2p/interface": "^2.10.5",
    "@libp2p/peer-id": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.7",
    "express": "^4.18.2",
    "helmet": "^8.0.0",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "pino-std-serializers": "^7.0.0"
  }
}